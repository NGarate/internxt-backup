{
  "extends": ["plugin:oxlint/recommended"],
  "rules": {
    "no-console": "off",
    "no-debugger": "error",
    "no-unused-vars": "error",
    "no-undef": "off",
    "no-empty": "error",
    "no-duplicate-imports": "error",
    "prefer-const": "error",
    "eqeqeq": ["error", "always"],
    "curly": ["error", "all"],
    "no-var": "error",
    "no-throw-literal": "error"
  }
}
